#include <iostream>
//#include <cmath>

using namespace std;

bool isPrime(unsigned int n){
  if(n==1||(n%2==0&&n!=2)){
    return false;
  }else if(n==2){
    return true;
  }else{
    for(unsigned int i=3;i<=n/2;i=i+2){
      if(n%i==0){
        return false;
      }
    }
  return true;
  }
}

int main(){

  for(unsigned int n=2;n<4294967295;n++) {
    if(isPrime(n)) cout<<n<<endl;
  }
  

  return 0;

}
